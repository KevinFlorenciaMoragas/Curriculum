@{
    <h2>Conocimientos</h2>
    <div class="container">
	<div class="row row-cols-1 row-cols-md-3 g-4">
		@foreach (var conocimientos in ViewBag.Vconocimientos)
		{
			<div class="col">
				<div class="card h-100">
					<img src="@conocimientos.imageURL" class="card-img-top" alt="imagen proyecto" />
					<div class="card-body">
						<h5 class="card-title">@conocimientos.name</h5>
						<p class="card-text">@conocimientos.description</p>
					</div>
				</div>
			</div>
		}
	</div>
</div>
}
